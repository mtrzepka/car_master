<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carmaster"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.carmaster.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.carmaster.GetStationWithPrices"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.carmaster.CarRecallActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.carmaster.RecallListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.carmaster.SingleRecallDetailActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.carmaster.CarValueActivity"
            android:label="@string/title_activity_car_value" >
        </activity>
    </application>

</manifest>
